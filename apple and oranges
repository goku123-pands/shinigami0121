m=list(map(int,input().strip().split()))
n=list(map(int,input().strip().split()))
o=list(map(int,input().strip().split()))
p=list(map(int,input().strip().split()))
q=list(map(int,input().strip().split()))
k=[]
l=[]
v=0
u=0
for i in p:
    k.append(i+n[0])
for j in q:
    l.append(j+n[1])
for a in k:
    if a>=m[0] and a<=m[1]:
        v+=1
for b in l:
    if b<=m[1] and b>=m[0]:
        u+=1
print(v)
print(u)
